{
    "dryRun": false,
    "tasks": [
      {
        "id": "checkVimInstalled",
        "condition": {
          "checkCommand": "vim --version",
          "expectedOutcome": "",
          "strategy": "OSCommandStrategy"
        },
        "strategy": "OSCommandStrategy",
        "dependsOn": []
      },
      {
        "id": "installVim",
        "command": "sudo dnf -y install vim",
        "dependsOn": ["checkVimInstalled"],
        "executeIf": "conditionFailed",
        "strategy": "OSCommandStrategy"
      },
      {
        "id": "printVimVersion",
        "command": "vim --version",
        "dependsOn": ["installVim"],
        "strategy": "OSCommandStrategy"
      }
    ]
  }
